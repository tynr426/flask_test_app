Mvtopic = {
	
	hasLoadCache : false,
	
	netDataCont : 0,
	
	mvInfo : null,
	
	loadingData : false,
	
	showNum : 0,
	
	addNum : 18,
	
	
	cacheData : {},
	
	newMvData : null,
	
	tpl : {
		mvItem : [
			'<li class="jumpclick"  page="mvtopic" hotclick="mv" recordid="%(id)" recordtype="%(type)" cid="%(cid)"  reportstr="2,200,27">',
				'<div class="mod_inner_box">',
					'<div class="mod_cover size_232_109"><img src="%(picUrl)"></div>',
					'<h3 class="mv_tit">%(title)</h3>',
					'<p class="mv_info">%(introduction)</p>',
					'<p><span class="date">%(date)</span><span class="count">%(listen)人播放</span></p>',
				'</div>',
			'</li>'
		].join(''),
		
		newMvItem : [
			'<div class="grid jumpclick"  page="mvtopic" hotclick="newmv" recordid="%(id)" recordtype="%(type)" reportstr="2,200,27">',
				'<div class="mod_media">',
					'<div class="mod_cover size_84"><img src="%(picurl)" /></div>',
					'<div class="media_body">',
						'<h3 class="media_heading">%(title)</h3>',
						'<p class="media_paragraph"><span class="song ellipsis">%(songname_0)</span><span class="singer">- %(singername_0)</span></p>',
						'<p class="media_paragraph"><span class="song ellipsis">%(songname_1)</span><span class="singer">- %(singername_1)</span></p>',
					'</div>',
				'</div>',
			'</div>'
		].join('')
	},
	
	getMvData : function(){
		
		if( this.loadingData ){ // 正在加载数据
			return; 
		}else{
			this.loadingData = true;
		}
		
		var mvInfo = this.mvInfo
		if( mvInfo && ( mvInfo.showNum >= mvInfo.count ) ){ // 加载完成
			M.hideLoading();
			return ;
		}else{
			M.showLoading();
			if( !Mvtopic.hasLoadCache ){
				Mvtopic.hasLoadCache = true;
				M.get('mvtopic.mvdata', function(ret){
					if(ret.code == 0){
						var data = "";
						
						try{
							data = JSON.parse( ret.value );
						}catch(e){
							data = null;
						}
						if(data){
							$(document.body).trigger('data:mvtopic', data);
						}
					}
				});
				
			}
			var url = 'http://i.y.qq.com/mv/fcgi-bin/fcg_mv_theme?cid=285&typeid=2009&itemid=1&sin='+this.showNum+'&pagesize='+ this.addNum +'&ct=18';
			this.loadingData = true;
			$.ajax({
				type	: 'get',
				url 	: url,
				dataType: 'jsonp',
				jsonp 	: 'callback',
				success	: function(data){
					Mvtopic.showNum += Mvtopic.addNum;
					Mvtopic.loadingData = false;
					Mvtopic.netDataCont++;
					if(Mvtopic.netDataCont == 1){
						M.set('mvtopic.mvdata', JSON.stringify(data), function(ret){});
					}
					$(document.body).trigger('data:mvtopic', data);
				}
			});
		}
	},
		
	getNewMvData : function(){
		M.get('mvtopic.newmvdata', function(ret){
			
			if(ret.code == 0){
				var data = "";
				
				try{
					data = JSON.parse( ret.value );
				}catch(e){
					data = null;
				}
				if(data){
					Mvtopic.newMvDataCallback(data);
				}
			}
			
		});
		
		setTimeout(function(){
			var url = 'http://proxy.music.qq.com/3gmusic/fcgi-bin/shoufa_cache.fcg?rettype=1&typeid=2001&itemid=1&sin=0&cid=284&ct=18&onlymv=1';
			$.ajax({
				type	: 'get',
				url 	: url,
				dataType: 'jsonp',
				jsonp 	: 'callback',
				success	: function(data){
					if(data.code == 0){
						M.set('mvtopic.newmvdata', JSON.stringify(data), function(ret){});
						Mvtopic.newMvDataCallback(data);
					}
				}
			});
		});
	},
	
	newMvDataCallback : function(data){
		if(data.code == 0){
			data.group = data.List[0];
			Mvtopic.newMvData = data;
			$(document.body).trigger('data:mvnew', data);
		}
	},
	
	initMvlist : function(event, data){
		var listhtml = [];
		$.each(data.list, function(index, item){
			item.listen = M.makeListenStr(item.listennum);
			
			item.title = item.title.decodeBase();
			item.introduction = item.introduction.decodeBase();
			item.date = item.date.decodeBase();
			item.listen = M.makeListenStr(item.listennum);
			item.picUrl = M.addMaxage(item.picUrl );
			
			listhtml.push(Mvtopic.tpl.mvItem.jstpl_format(item));
		});
		if( Mvtopic.netDataCont < 2 ){
			$('.width_item_232').html(listhtml.join(''));
		}else{
			$('.width_item_232').append(listhtml.join(''));
		}
		
		var s = Mvtopic.speed;
		if( !s.pageEnd ){
			s.pageEnd = (+ new Date());
			setTimeout(function(){
				M.doSpeed('&flag3=50&10='+ (s.pageEnd - s.pageStart) );
			});
		}
	},
	
	initNewMvlist : function(event, data){
		var listhtml = [];
		
		$.each(data.group.item, function(index, item){
			item.listen = M.makeListenStr(item.listennum);
			
			item.singername_0 = item.singername_0.decodeBase();
			item.singername_1 = item.singername_1.decodeBase();
			item.songname_0 = item.songname_0.decodeBase();
			item.songname_1 = item.songname_1.decodeBase();
			item.title = item.title.decodeBase();
			item.picurl = M.addMaxage(item.picurl );
			
			listhtml.push(Mvtopic.tpl.newMvItem.jstpl_format(item));
		});
		
		$('.layout_column_2').html(listhtml.join(''));
	},
	
	speed : {
		pageEnd : 0,
		pageStart : 0
	},
	
	init : function(){
		this.speed.pageStart = (+ new Date());
		$(document).on('data:mvtopic', Mvtopic.initMvlist);
		$(document).on('data:mvnew', Mvtopic.initNewMvlist);
		
		$(document).on('scroll', function(){
			var bodyHeight = document.body.clientHeight,
				clientHeight = window.innerHeight,
				maxScroll = bodyHeight-clientHeight,
				scrollTop = document.body.scrollTop;
			if( scrollTop >= maxScroll-100 && !Mvtopic.loadingData ){
				Mvtopic.getMvData();
			}
		}, false);
		
		$(document.body).slideToucher({
			bindClass : '.jumpclick',
			clickCb : function(event, bindNode, shareDate){
				var recordtype  = parseInt($(bindNode).attr('recordtype')),
					recordid	= $(bindNode).attr('recordid'),
					hotpage = $(bindNode).attr('page')||'defualt',
					hottype = $(bindNode).attr('hotclick')||'defualt';
				var reportstr = $(bindNode).attr('reportstr')||'';
				M.hotClick('ipad.'+hotpage+'.'+hottype);
				if( recordtype && recordid ){
					 M.jump(recordtype, recordid, reportstr);
				}
			}
		});
		
		Mvtopic.getNewMvData();
		
		setTimeout(function(){
			Mvtopic.getMvData();
		},100);
		
		setTimeout(function(){
			M.doPv('/ipad/mvtopic.html');
		}, 1000);
	}
	
};

document.addEventListener("DOMContentLoaded", function(){

	Ipad.init();
	setTimeout(function(){
		Mvtopic.init();
	},100);
}, false);

</script>
</body>
</html>