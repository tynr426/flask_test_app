ADFOR.itemSpace= 15;
ADFOR.space = 405;
try{
    document.execCommand("BackgroundImageCache", false, true);
}catch(e){}

DSPUIBase.mix(ADFOR,{
    weightArray: [],
    
    onRenderContentCallback: function(data){
        
        var cache = {},
            sortArray = [];

        for(var i=0,l=data.length;i< l;i++){
            var item = data[i];
            sortArray.push(item);
            cache[item.title] = i;
        }

        sortArray.sort(function(a, b){
            var weightA = parseInt(a.weight || '0', 10);
            var weightB = parseInt(b.weight || '0', 10);

            return weightA > weightB ? -1 : 1;
        });
        this.weightArray[0] = cache[sortArray[0].title] || 0;
        this.weightArray[1] = cache[sortArray[1].title] || 0;
        
    },
    resetWeight: function(){
        

        var links = this.panel.find('a');
        
        links.eq(this.weightArray[0]).addClass('hot_two');
        links.eq(this.weightArray[1]).addClass('hot_two');
        
    },
    onRestCallback: function(){
        var self = this;
        
        self.resetWeight();
        
    },
    onSwitchCallback: function(){
        var self = this;
        
        self.resetWeight();
        
    }
});




ADFOR.ready(function(){
    try {
        ADFOR.render();
    }catch(e){
        ADFOR.error('rendererror');
    }
    ADFOR.loadlog('loadsuccess');
